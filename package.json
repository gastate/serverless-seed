    "@types/mocha": "^2.2.33",
    "@types/node": "0.0.2",
    "expect.js": "^0.3.1",
    "karma": "^1.2.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-typescript": "^2.1.4",
