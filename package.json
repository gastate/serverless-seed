{
  "name": "petshop",
  "version": "0.0.1",
  "description": "Sample project",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && rm -rf coverage && rm -rf testreports && rm -rf .serverless && rm -rf .sonar && npm install",
    "globals": "npm install -g serverless mocha typescript typings istanbul",
    "prestart": "npm run clean && tsc",
    "start": "node --debug `which serverless` offline start",
    "predeploy": "npm run clean && tsc && npm prune --production",
    "deploy": "sls deploy",
    "precover": "npm run clean && tsc",
    "cover": "istanbul cover node_modules/.bin/_mocha -- -- -u exports -R spect --require build/test/index.js build/test/**/*.js",
    "pretest": "npm run clean && tsc",
    "test": "mocha --recursive --require build/test/index.js build/test",
    "pretestdebug": "npm run clean && tsc",
    "testdebug": "mocha --recursive --require build/test/index.js --debug-brk build/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkolberg/serverless-seed.git"
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": "https://github.com/kkolberg/serverless-seed/issues"
  },
  "homepage": "https://github.com/kkolberg/serverless-seed#readme",
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^2.2.33",
    "@types/node": "0.0.2",
    "expect.js": "^0.3.1",
    "mocha": "^3.1.2",
    "remap-istanbul": "^0.8.0",
    "temp": "^0.8.3",
    "typescript": "latest",
    "typings": "^2.0.0"
  },
  "dependencies": {
    "app-module-path": "^2.1.0",
    "serverless-offline": "^3.5.7"
  }
}
